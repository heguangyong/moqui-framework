{
  "name": "novel-anime-generator",
  "version": "1.0.0",
  "description": "Novel to Anime Video Generator - AI-powered system for converting novels into animated short videos",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.0",
    "axios": "^1.7.0",
    "pinia": "^2.2.0",
    "quasar": "^2.18.0",
    "vue": "^3.5.0",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.8.0",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.1.0",
    "@vitest/ui": "^2.1.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.28.0",
    "fast-check": "^3.22.0",
    "jsdom": "^27.3.0",
    "sass": "^1.80.0",
    "typescript": "~5.9.0",
    "vite": "^5.4.0",
    "vitest": "^2.1.0",
    "vue-tsc": "^2.1.0"
  }
}
